{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "risunosu",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"commit": "git-cz",
		"check": "concurrently \"npm:check:*(!tsc:)\"",
		"check:biome": "biome check --apply --error-on-warnings .",
		"check:prettier": "prettier --write --cache .",
		"check:tsc": "concurrently npm:check:tsc:*",
		"check:tsc:base": "tsc --project tsconfig.base.json",
		"check:tsc:src": "tsc --project tsconfig.src.json",
		"check:cspell": "cspell --cache \"**/*\"",
		"check:knip": "knip",
		"ignore-sync": "ignore-sync .",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/cache": "11.11.0",
		"@emotion/react": "11.11.4",
		"@emotion/styled": "11.11.5",
		"@mui/icons-material": "5.15.16",
		"@mui/material": "5.15.16",
		"@mui/material-nextjs": "5.15.11",
		"@vercel/analytics": "1.2.2",
		"@vercel/speed-insights": "1.0.10",
		"next": "14.2.3",
		"react": "18.3.1",
		"react-dom": "18.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.2",
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@commitlint/cz-commitlint": "19.2.0",
		"@commitlint/types": "19.0.3",
		"@cspell/cspell-types": "8.8.0",
		"@tsconfig/bun": "1.0.5",
		"@tsconfig/next": "2.0.3",
		"@tsconfig/strictest": "2.0.5",
		"@types/bun": "1.1.1",
		"@types/lint-staged": "13.3.0",
		"@types/react": "18.3.1",
		"@types/react-dom": "18.3.0",
		"commitizen": "4.3.0",
		"concurrently": "8.2.2",
		"cspell": "8.8.0",
		"husky": "9.0.11",
		"ignore-sync": "7.0.1",
		"knip": "5.12.2",
		"lint-staged": "15.2.2",
		"prettier": "3.2.5",
		"typescript": "5.4.5"
	},
	"trustedDependencies": ["@biomejs/biome"],
	"volta": {
		"node": "21.7.3"
	}
}
